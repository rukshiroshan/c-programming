#include <stdio.h>
#include <stdlib.h>

/* Temperature of a city in fehrenite degree is input through the keyboard.write a program to convert this temperature into centegrades degree*/

int main()
{
    float f,c;
    printf("\nEnter the temperature:  ");
    scanf("%f",&f);
    c=(f-32)*(5.0/9.0);
    printf("%0.2f is in cntegrates",c);
    return 0;
}

#include <stdio.h>
#include <stdlib.h>

/*Two numbers are input through the keyboard into 2 locations C and D .write a program to interchange the contents of C and D*/

int main()
{
    int C,D,E;
    printf("\n enter 2 numbers:");
    scanf("%d%d",&C,&D);
    printf("\n C:%d  D:%d",C,D);
    E=C;
    C=D;
    D=E;
    printf("\n C:  %d    D:  %d",C,D);

    return 0;
}
